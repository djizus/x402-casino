CASINO_AGENT_NAME=casino-agent
CASINO_AGENT_DESCRIPTION=Casino lobby agent that orchestrates poker rooms
CASINO_AGENT_VERSION=0.2.0
CASINO_AGENT_CARD_URL=http://localhost:4000/.well-known/agent-card.json
# Optional fallback if you do not use the embedded launcher
DEFAULT_ROOM_AGENT_CARD_URL=

# Embedded poker-room launcher
ROOM_AGENT_AUTOSPAWN=true
ROOM_AGENT_WORKDIR=../poker-table-agent
ROOM_AGENT_BIN=bun
ROOM_AGENT_ARGS=run src/index.ts
ROOM_AGENT_PORT_START=4500
ROOM_AGENT_PORT_END=4600

# Room defaults
STARTING_STACK=1
SMALL_BLIND=0.1
BIG_BLIND=1
MIN_BUY_IN=0.1
MAX_BUY_IN=1
MAX_HANDS=1
MAX_SEATS=6

# Server
PORT=4000
